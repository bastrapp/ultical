{
  "name": "ultical",
  "version": "0.1.0",
  "private": true,
  "description": "Ultimate Frisbee Calendar",
  "keywords": [
    "ultimate",
    "ultimate frisbee",
    "calendar"
  ],
  "author": "bas <sebastian.trapp@gmail.com>",
  "contributors": [
    "kaffeetrinkr (https://github.com/kaffeetrinkr)",
    "nuest (http://github.com/nuest)"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastrapp/ultical.git"
  },
  "bugs": {
    "url": "https://github.com/bastrapp/ultical/issues"
  },
  "homepage": "https://www.dfv-turniere.de",
  "main": "components/app.js",
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "three": "global:THREE"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-moment": "^0.10.3",
    "angular-motion": "^0.4.3",
    "angular-sanitize": "^1.5.0",
    "angular-slugify": "^1.0.3",
    "angular-strap": "^2.3.7",
    "angular-translate": "^2.9.2",
    "angular-ui-router": "^0.2.18",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.1",
    "jsog": "^1.0.7",
    "respond": "^0.9.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-header": "^0.9.2",
    "browserify-shim": "^3.8.12",
    "clean-css": "^3.4.9",
    "jshint": "^2.9.1",
    "minifyify": "^7.3.2",
    "ncp": "^2.0.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1"
  },
  "scripts": {
    "lint": "jshint components/**.js public/**.js",
    "clean": "rimraf dist/* dist/.htaccess.dist",
    "prebuild": "npm run clean -s",
    "build:public": "ncp public dist",
    "watch:public": "watch 'npm run build:public' public/",
    "build:js": "browserify components/**.js --no-bundle-external -d -p [minifyify --map dist/ultical.js.map --output dist/ultical.js.map] -p [browserify-header] -o dist/ultical.min.js",
    "build:jsbundle_BACKUP": "browserify --entry components/*.js public/**/*.js -d -p [minifyify --map dist/bundle.js.map --output dist/bundle.js.map] -p [browserify-header] -o dist/bundle.min.js",
    "build:jsbundle": "browserify components/*.js public/**/*.js -d -o dist/bundle.min.js",
    "watch:jsbundle": "watch 'npm run build:jsbundle' components/",
    "build:css": "cat css/*.css | cleancss -o dist/ultical.css",
    "watch:css": "watch 'npm run build:css' css/",
    "build": "npm run build:public && npm run build:css && npm run build:jsbundle",
    "build:watch": "parallelshell 'npm run watch:jsbundle' 'npm run watch:css' 'npm run watch:public'",
    "debug": "browserify components/**.js > dist/app.js",
    "test": "echo \"Error: no tests specified yet\" && exit 1"
  }
}
